generator client {
  provider      = "prisma-client"
  output        = "../app/generated/prisma"
  binaryTargets = ["native", "windows"]
}

datasource db {
  provider = "mysql"
}

model ticket {
  id          String        @id @default(cuid())
  title       String
  description String?
  status      ticket_status @default(TODO)
  assignedTo  String
  createdAt   DateTime      @default(now())
  updatedAt   DateTime
}

enum ticket_status {
  TODO
  IN_PROGRESS
  DONE
  REJECTED
}
